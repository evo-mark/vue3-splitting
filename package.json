{
	"name": "vue3-splitting",
	"version": "1.1.1",
	"author": "southcoastweb",
	"homepage": "https://southcoastweb.co.uk",
	"scripts": {
		"dev": "vite",
		"build": "vite build && tsc --emitDeclarationOnly",
		"preview": "vite preview",
		"test:unit": "vitest --dom",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"prepublishOnly": "npm run lint && npm run build"
	},
	"main": "./dist/vue3-splitting.js",
	"module": "./dist/vue3-splitting.mjs",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/vue3-splitting.js",
			"require": "./dist/vue3-splitting.mjs",
			"types": "./dist/types/index.d.ts"
		},
		"./styles": "./dist/style.css"
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"vue": "^3.2.0"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.2.0",
		"@testing-library/vue": "^6.6.1",
		"@types/jsdom": "^21.1.0",
		"@types/node": "^18.14.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/test-utils": "^2.3.0",
		"@vue/tsconfig": "^0.1.3",
		"eslint": "^8.34.0",
		"eslint-plugin-vue": "^9.9.0",
		"happy-dom": "^8.9.0",
		"jsdom": "^21.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.4",
		"typescript": "~4.7.4",
		"vite": "^4.1.3",
		"vite-plugin-dts": "^2.0.2",
		"vitest": "^0.29.1",
		"vue-tsc": "^1.1.5"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"bugs": {
		"url": "https://github.com/south-coast-web/vue3-splitting/issues"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/south-coast-web/vue3-splitting.git"
	},
	"keywords": [
		"Vue",
		"Vue3",
		"splitting",
		"animation",
		"words",
		"separate",
		"characters"
	]
}
