{
	"name": "vue3-splitting",
	"version": "2.0.1",
	"author": "evoMark",
	"homepage": "https://evomark.co.uk",
	"scripts": {
		"dev": "vite --host",
		"build": "vite build && tsc --emitDeclarationOnly",
		"test:unit": "vitest --dom",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"prepublishOnly": "npm run lint && npm run build"
	},
	"main": "./dist/vue3-splitting.cjs",
	"module": "./dist/vue3-splitting.js",
	"browser": "./dist/vue3-splitting.umd.cjs",
	"type": "module",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/vue3-splitting.js",
			"require": "./dist/vue3-splitting.cjs",
			"browser": "./dist/vue3-splitting.umd.cjs",
			"types": "./dist/types/index.d.ts"
		},
		"./styles": "./dist/style.css"
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"vue": "^3.3.0"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.7.2",
		"@testing-library/vue": "^8.0.2",
		"@types/jsdom": "^21.1.6",
		"@types/node": "^20.11.24",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/test-utils": "^2.4.4",
		"@vue/tsconfig": "^0.5.1",
		"@vueuse/core": "^10.9.0",
		"eslint": "^8.57.0",
		"eslint-plugin-vue": "^9.22.0",
		"happy-dom": "^13.6.2",
		"jsdom": "^24.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.2.5",
		"typescript": "^5.3.3",
		"vite": "^5.1.5",
		"vite-plugin-dts": "^3.7.3",
		"vitest": "^1.3.1",
		"vue-tsc": "^2.0.5"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"bugs": {
		"url": "https://github.com/evo-mark/vue3-splitting/issues"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/evo-mark/vue3-splitting.git"
	},
	"keywords": [
		"Vue",
		"Vue3",
		"splitting",
		"animation",
		"words",
		"separate",
		"characters"
	]
}
